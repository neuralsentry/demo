{
  "name": "demo",
  "version": "0.1.0",
  "description": "NeuralSentry Demonstration FYP",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "db:generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "db:migrate": "ts-node -r tsconfig-paths/register bin/migrate.ts",
    "db:upgrade": "npm run db:generate && npm run db:migrate",
    "db:downgrade": "npm run db:drop && npm run db:migrate",
    "db:clean": "ts-node -r tsconfig-paths/register bin/clean.ts",
    "db:seed": "ts-node -r tsconfig-paths/register bin/seed.ts",
    "db:reset": "npm run db:clean && npm run db:seed",
    "db:setup": "npm run db:upgrade && npm run db:seed",
    "db:studio": "drizzle-kit studio",
    "build": "tsc",
    "lint:eslint": "eslint src/**/*.ts",
    "lint:watch": "npx tsc --watch --noEmit",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "express",
    "typescript",
    "nodejs",
    "twitter-clone"
  ],
  "author": "cNoside",
  "license": "MIT",
  "dependencies": {
    "argon2": "0.30.3",
    "drizzle-orm": "0.27.1",
    "express": "^5.0.0-beta.1",
    "express-rate-limit": "6.9.0",
    "http-errors": "2.0.0",
    "http-status": "1.6.2",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "postgres": "3.3.5",
    "winston": "3.9.0",
    "zod": "3.22.1"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/lodash": "4.14.195",
    "@types/morgan": "1.9.4",
    "@types/node": "20.4.1",
    "@types/pg": "8.10.2",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "concurrently": "8.2.0",
    "dotenv": "16.3.1",
    "drizzle-kit": "0.19.3",
    "eslint": "8.44.0",
    "pg": "8.11.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  }
}
